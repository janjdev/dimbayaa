	body(class="reg-body")
		<!-- Preloader--------------------------------------->
		div(id="loader-wrapper")
			div(id="loader")
				img(src="img/logo.png")
			div(class="loader-section section-left")
			div(class="loader-section section-right")
		//Primary Page Layout --------------------------------------
		div(class="container")
			div(class="row")
				div(class="col-md-12 nav")
					div(class="col-xs-6 col-md-4 name")
						a(href="index.html")
							img(src="img/dimbayaa-color.png" alt="Dimabayaa Arts" title="Dimbayaa Arts")
					ul(class="col-xs-12 col-md-8 nav-list")
						li: a(href="mission.html") VISION
						li: a(href="projects.html") PROJECTS
						li: a(href="about.html") ABOUT
						li: a(class="active" href="https://mocha3019.mochahost.com/~brklyn31/dimbayaa.art/reg.html") SummerWorkshop
			div(class="row headliner")
				div(class="col-xs-12 col-md-4 page-title")
					h1 SUMMER WORKSHOP 17
				div(class="col-xs-12 col-md-8 quote")
					p To take part in the African revolution, it is not enough to write a revolutionary song. You must fashion the revolution with the people. And if you fashion it with the people, the songs will come by themselves. ~ Sekou Toure
			div(class="wedo-main")
				div(class="row")
					h4(class="subtitle") How about we #[span(class="orange") DANCE?] 
					p(class="subtitle") Moving forward on our path to facilitate restorative healing and joyous self-expression, we invite you to catch us on the dance floor!!
				div(class="row workshop")
					div(class="col-xs-12 col-md-6 col-lg-6 flyer")
						img(src="img/projects/workshops/summer2017/Class-Flyer-1.3.png" alt="African Dance Summer Workshop" title="Dimbayaa Arts African Dance Summer Workshop 2017")
					div(class="col-xs-12 col-md-6 workshop_name")
						div(class="row")
							div(class="page-title")
								h1 Dimbayaa Arts Summer Workshop
					div(class="col-xs-12 col-md-6 workshop_details")
						li
							div(class="col-xs-4") #[p DATES:] 
							div(class="col-xs-8") #[p July 26th - August 30th]
						li
							div(class="col-xs-4") #[p TIME:] 
							div(class="col-xs-8") #[p WEDNESDAYS 6:30PM - 8PM]
						li
							div(class="col-xs-4") #[p CHILDREN:] 
							div(class="col-xs-8") #[p $5 #[span 12 and under]]
						li
							div(class="col-xs-4") #[p ADULTS:] 
							div(class="col-xs-8") #[p $7 #[span 13 and up]]
						li
							div(class="col-xs-4") #[p LOCATION:] 
							div(class="col-xs-8") #[p #[a(href="https://www.google.com/maps/place/14th+Street+Artist+Community%2FZuka+Arts+Guild+Gallery%2FKahrizma's+Art/@38.65051,-90.1998537,17z/data=!3m1!4b1!4m5!3m4!1s0x87d8b332f47ef14f:0xdeecc267fe6a902d!8m2!3d38.65051!4d-90.197665" target="blank") 2701 N. 14th Street, 63106] ]
					div(class="col-xs-12 col-md-12 col-lg-6 registration")
						h4 Early registration ends midnight July 25th
						form(id="reg_form")
							fieldset
								legend Workshop Registration
								p(class="col-xs-6 selection") 
									label Adult
									select(id="ap" class="selector")
										option(value="0") select
										option(value="1") 1
										option(value="2") 2
										option(value="3") 3 
										option(value="4") 4
								p(class="col-xs-6 selection") 
									label Child
									select(id="cp" class="selector")
										option(value="0") select 
										option(value="1") 1
										option(value="2") 2
										option(value="3") 3 
										option(value="4") 4
								button( id="btn" class="btn btn-secondary btn-lg btn-block" type ="button" value="Register" form="reg_form") REGISTER NOW
				div(id="registrations_forms" class="row")
					div(id="site")
						form(id="reg_info" class="col-xs-12 hidden" method="post" action="order.html")
							fieldset(id="#fieldset-billing")
								legend Enter registrant information
								div(id="billing")
									h3(class="label") Billing Information
									div(id="billingForm" class="billing_group")
										div(class="col-xs-6 col-md-4")
											input(type="text" name="fname" required="required" placeholder="First Name" class="col-xs-12 billing_input valid" data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-6 col-md-4")
											input(type="text" name="lname" required="required" placeholder="Last Name" class="col-xs-12 billing_input valid" data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-6 col-md-4")
											input(type="email" name="email" required="required" placeholder="Email" class="col-xs-12 billing_input valid")
										div(class="col-xs-6 col-md-3")
											input(type="tel" pattern="^(?:\\\(\\\d{3}\\\)|\\\d{3})[- ]?\\\d{3}[- ]?\\\d{4}$" name="phone" required="required" placeholder="Phone" class="col-xs-12 billing_input valid" title="Please enter a phone number." data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-6 col-md-3")
											input(type="text" name="apt" placeholder="Apartment" class="col-xs-12 billing_input" data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-6 col-md-5")
											input(type="text" name="street" required="required" placeholder="Street Number" class="col-xs-12 billing_input valid" pattern="^\\\d+\\\s[A-z]+\\\s[A-z]+" data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-6 col-md-5")
											input(type="text" name="city" required="required" placeholder="city" class="col-xs-12 billing_input valid" data-type="string" data-message="This field cannot be empty")
										div(class="col-xs-4 col-md-2 right")
											select(id="state" class="col-xs-12 billing_input valid" required="required" data-type="string" data-message="This field cannot be empty")
												option(value="") ST
												option(value="AL") AL
												option(value="AK") AK
												option(value="AZ") AZ
												option(value="AR") AR
												option(value="CA") CA
												option(value="CO") CO
												option(value="CT") CT
												option(value="DE") DE
												option(value="DC") DC
												option(value="FL") FL
												option(value="GA") GA
												option(value="HI") HI
												option(value="ID") ID
												option(value="IL") IL
												option(value="IN") IN
												option(value="IA") IA
												option(value="KS") KS
												option(value="KY") KY
												option(value="LA") LA
												option(value="ME") ME
												option(value="MD") MD
												option(value="MA") MA
												option(value="MI") MI
												option(value="MN") MN
												option(value="MS") MI
												option(value="MO") MO
												option(value="MT") MT
												option(value="NE") NE
												option(value="NV") NV
												option(value="NH") NH
												option(value="NJ") NJ
												option(value="NM") NM
												option(value="NY") NY
												option(value="NC") NC
												option(value="ND") ND
												option(value="OH") OH
												option(value="OK") OK
												option(value="OR") OR
												option(value="PA") PA
												option(value="RI") RI
												option(value="SC") SC
												option(value="SD") SD
												option(value="TN") TN
												option(value="TX") TX
												option(value="UT") Utah
												option(value="VT") VT
												option(value="VA") VA
												option(value="WA") WA
												option(value="WV") WV
												option(value="WI") WI
												option(value="WY") WY
										div(class="col-xs-6 col-md-3 last")
											input(type="text" name="zipcode" required="required" placeholder="zip-code" class="col-xs-12 billing_input valid" pattern="^\\\d{5}(?:[-\\\s]\\\\d{4})?$" data-type="string" data-message="This field cannot be empty")
										span(class="float")
								div(id="adultParameters")
									h3(class="label") Adults +13
										i(class="ad fa fa-plus")
										i(class="ad fa fa-minus")
										div
											label(for="qty-1") Quantity
											input(type="text" name="qty-1" id="qty-1" class="qty" value="0" disabled)
									div(id="adforms"  class="product")
										h3(class="label")
								div(id="childParameters")
									h3(class="label") Children #[span 12 &amp; under]
										i(class="ch fa fa-plus")
										i(class='ch fa fa-minus')
										div
											label(for="qty-1") Quantity
											input(type="text" name="qty-1" id="qty-2" class="qty" value="0" disabled)
									div(id="gaurdian")
										h3(class="label gaurdian")
									div(id="chforms"  class="product")
										h3(class="label Children-label")
								button(id="order-button" class="btn btn-primary btn-lg btn-block" type="submit" value="" form="reg_info") SUBMIT
		div(class="footer")
			div(class="container")
				div(clas="row")
					div(class="col-md-12 nav")
						div(class="col-md-5 name-footer")
							a #[img(src="img/dimbayaa-color.png" alt="Dimbayaa Arts" title="Dimbayaa Arts")]
						ul(class="col-md-7 nav-list")
							li: a(href="mission.html") VISION
							li: a(href="projects.html") PROJECTS
							li: a(href="about.html") ABOUT
							li: a(class="active" href="https://mocha3019.mochahost.com/~brklyn31/dimbayaa.art/reg.html") SummerWorkshop
							
